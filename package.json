{
  "name": "mern-passportjwt",
  "version": "1.0.0",
  "description": "MERN stack and passport jwt strategy",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AngelMD55/MERN-PassportJWT.git"
  },
  "author": "Angel Demerutis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AngelMD55/MERN-PassportJWT/issues"
  },
  "homepage": "https://github.com/AngelMD55/MERN-PassportJWT#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "react-router-dom": "^5.0.0"
  }
}
